#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int> nums(n);
        for(int i=0;i<n;i++){
            cin>>nums[i];
        }
        unordered_map<int, int> hash;
        for(int num : nums){
            hash[num]++;
        }
        vector<int> ans;
        int maxFreq = 0;
        for(auto it : hash){
            maxFreq = max(maxFreq, it.second);
        }
        for(auto it : hash){
            if(it.second == maxFreq){
                ans.push_back(it.first);
            }
        }
        cout<<*min_element(ans.begin(), ans.end())<<endl;
    }
    return 0;
}
