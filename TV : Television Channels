#include <bits/stdc++.h>

using namespace std;

int main() {
    int t = 1;
    // cin >> t;

    while(t--){
        int a;
        cin >> a;
                   
        if(a % 2 == 0)
            cout << a / 2 << endl;
        else    
            cout << (a / 2) + 1 << endl;
    }

    return 0;
}TV
