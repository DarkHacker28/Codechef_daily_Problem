#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    string n;
    cin>>n;
    int c=0,a=0,t=0;
    for(int i=0;i<3;i++){
        if(n[i]=='c') c++;
        if(n[i]=='a') a++;
        if(n[i]=='t') t++;
    }
    if(c==a && c==t && c==1 && n.length()==3){
        cout<<"yes";
    }
    else cout<<"no";

}
