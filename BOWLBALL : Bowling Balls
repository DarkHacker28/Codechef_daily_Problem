#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t; cin>>t;
    while(t--){
        int n,x,y; cin>>n>>x>>y;
        int arr[n];
        int c=0;
        for(int i=0;i<n;i++){
            cin>>arr[i];
            if(x<=arr[i] && arr[i]<=y)
                c++;
        }
        cout<<c<<endl;
    }
    return 0;
}
