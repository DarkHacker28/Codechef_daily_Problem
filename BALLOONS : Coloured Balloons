#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int arr[n];
        for(int i = 0 ; i < n ; i++){
            cin>>arr[i];
        }
        int cost = 0 ;
        for(int i = 0 ; i < n ; i++){
            cost = cost + (i+1)*arr[i] ;
        }
        cout<<cost<<endl;
    }
    return 0;
}
