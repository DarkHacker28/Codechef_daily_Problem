#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int evenDivisor=0,oddDivisor=0;
        for(int i=1;i<=n;i++) {
            if (n%i==0) {
                if ((i%2)==0)
                    evenDivisor++;
                else
                    oddDivisor++;
            }
        }
        cout <<oddDivisor<<" "<<evenDivisor<< endl;
    }
    return 0;
}
