#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	
    cin >> T;
    for(int i=1; i<=T; i++){
        int N;
        cin >> N;
        int lowPrice= INT_MAX;
        for(int i=0; i<N; i++){
            int A,B;
            cin >> A >> B;
            if(A>=7){
                if(B < lowPrice){
                    lowPrice = B;
                }
            }
        }
        if(lowPrice == INT_MAX){
                cout << "-1" << endl;
            }
            else{
                cout << lowPrice << endl;
            }
    }
}
