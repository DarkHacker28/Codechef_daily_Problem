#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int c1 = 0, c2 = 0; // Initialize counts for red and blue
        for (int i = 0; i < n; i++) {
            int color;
            cin >> color;
            if (color == 1) {
                c1++;
            } else if (color == 2) {
                c2++;
            }
            // We don't need to explicitly count 0s,
            // as they can be used to balance the counts
        }

        if (n % 2 != 0) {
            cout << "No" << endl;
        } else {
            if (c1 > n / 2 || c2 > n / 2) {
                cout << "No" << endl;
            } else {
                cout << "Yes" << endl;
            }
        }
    }
    return 0;
}
