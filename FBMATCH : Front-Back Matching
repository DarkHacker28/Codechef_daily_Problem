#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string s;
	    cin>>s;
	    unordered_map<char, int> hash;
	    for(char ch : s){
	        hash[ch]++;
	    }
	    bool ans = false;
	    for(auto it: hash){
	        if(it.second == 2){
	            ans = true;
	        }
	    }
	    cout<<(ans ? "Yes" : "No")<<endl;
	}
	return 0;
}
